DROP TABLE IF EXISTS credit_card;  
	

create table credit_card (
       id bigint not null,
       name varchar(255),
		 cardNumberDetails varchar(255),
		 limitValue bigint,
		   balance_value bigint,
        primary key (id)
    );
	
